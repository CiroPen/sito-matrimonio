{
  "baseURL": "https://ciropen.github.io",
  "languageCode": "en-us",
  "title": "Valentina e Ciro",
  "show_comments": false,
  "disableKinds": [
    "sitemap",
    "RSS"
  ],
  "markup": {
    "defaultMarkdownHandler": "goldmark",
    "goldmark": {
      "extensions": {
        "definitionList": true,
        "footnote": true,
        "linkify": true,
        "linkifyProtocol": "https",
        "strikethrough": true,
        "table": true,
        "taskList": true,
        "typographer": true
      },
      "parser": {
        "attribute": {
          "block": false,
          "title": true
        },
        "autoHeadingID": true,
        "autoHeadingIDType": "github"
      },
      "renderer": {
        "hardWraps": false,
        "unsafe": true,
        "xhtml": false
      }
    }
  },
  "params": {
    "background_image_1": "https://i.imgur.com/7VruBfz.jpeg",
    "background_image_2": "https://i.imgur.com/NvL5IrL.jpeg",
    "background_image_3": "https://i.imgur.com/hZoHB4j.jpeg",
    "background_image_4": "https://i.imgur.com/80sHRUX.jpeg",
    "background_image_5": "https://i.imgur.com/QyYiySM.jpeg"
  }
}
